{
  "name": "@rosmarinus/nest-core",
  "version": "1.2.0",
  "description": "nest core modules for rosmarinus projects",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "rollup -c rollup.config.mjs -w",
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "release": "rosmarinus-publish publish",
    "test:unit": "jest --runInBand",
    "prepare": "husky install"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rosmarinus-project/nest-core.git"
  },
  "keywords": [
    "nest",
    "rosmarinus"
  ],
  "license": "MIT",
  "dependencies": {
    "@rosmarinus/common-utils": "^1.4.0",
    "ioredis": "^5.3.2",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.2.8",
    "express": ">=4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@nestjs/common": "^10.2.8",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rosmarinus/eslint-config-rosmarinus": "^1.0.1",
    "@rosmarinus/local-publish-tool": "^1.0.8",
    "@types/express": "^4.17.20",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.14.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^27.4.7",
    "jest-json-reporter": "^1.2.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.3.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.9.0"
}
